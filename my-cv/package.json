{
  "name": "my-cv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aos": "*",
    "bootstrap": "*",
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "sass:watch": "./node_modules/node-sass/bin/node-sass -w src/styles/scss -o src/styles/css --source-map true",
    "sass:build": "./node_modules/node-sass/bin/node-sass src/styles/scss -o src/styles/css --output-style compressed",
    "start": "yarn run sass:build && react-scripts start",
    "build": "yarn run sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "node-sass": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
